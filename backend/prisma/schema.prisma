generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model SensorData {
  id  Int @id @default(autoincrement())
  curah_hujan Float
  ketinggian_air Float
  suhu_udara Float
  kecepatan_angin Float
  status_hujan String
  status_air String
  status_suhu String
  status_angin String
  createdAt   DateTime @default(now())
}

model User {
  id Int @id @default(autoincrement())
  username String @unique
  password String
  createdAt DateTime @default(now())
}
